    "def gradient_optimization(\n",
    "    f,\n",
    "    f_derivative,\n",
    "    learning_rate=0.05,\n",
    "    epochs=20,\n",
    "    x_i=4\n",
    "):\n",
    "    x_values = []\n",
    "    y_values = []\n",
    "\n",
    "    for _ in range(epochs):\n",
    "        u = f_derivative(x_i)\n",
    "        x_i = x_i - learning_rate * u\n",
    "        x_values.append(x_i)\n",
    "        y_values.append(f(x_i))\n",
    "    \n",
    "    return x_i, x_values, y_values"
   ]
